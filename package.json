{
    "name": "eslint-plugin-code-style",
    "version": "2.2.1",
    "description": "A custom ESLint plugin for enforcing consistent code formatting and style rules in React/JSX projects",
    "main": "dist/index.js",
    "types": "index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist/index.js",
        "index.d.ts",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "node esbuild.config.js"
    },
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin",
        "code-style",
        "formatting",
        "react",
        "jsx",
        "linting",
        "code-quality"
    ],
    "author": "Mohamed Elhawary",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Mohamed-Elhawary/eslint-plugin-code-style.git"
    },
    "bugs": {
        "url": "https://github.com/Mohamed-Elhawary/eslint-plugin-code-style/issues"
    },
    "homepage": "https://github.com/Mohamed-Elhawary/eslint-plugin-code-style#readme",
    "peerDependencies": {
        "eslint": ">=9.0.0"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "devDependencies": {
        "esbuild": "^0.27.3"
    }
}
